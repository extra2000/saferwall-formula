[backend]
address = 'http://127.0.0.1:{{ backend.port.host }}'
admin_user = "{{ backend.admin.user }}"
admin_pwd = "{{ backend.admin.password }}"

[nsq]
lookupd = ["nsq-pod.saferwall:{{ nsq.nsqlookup.port.http.host }}"]
address = 'nsq-pod.saferwall:{{ nsq.port.tcp.host }}'

[minio]
endpoint = 'minio-pod.saferwall:{{ minio.port.host }}'
accesskey = '{{ minio.accesskey }}'
seckey = '{{ minio.seckey }}'
spacename = 'samples'
ssl = false

[multiav]
  [multiav.avast]
  enabled = false
  addr = "multiav-pod.saferwall:10001"

  [multiav.avira]
  enabled = false
  addr = "multiav-pod.saferwall:10002"

  [multiav.bitdefender]
  enabled = false
  addr = "multiav-pod.saferwall:10003"

  [multiav.clamav]
  enabled = {{ multiav.clamav.enabled | string | lower }}
  addr = "multiav-pod.saferwall:{{ multiav.clamav.port.host }}"

  [multiav.comodo]
  enabled = {{ multiav.comodo.enabled | string | lower }}
  addr = "multiav-pod.saferwall:{{ multiav.comodo.port.host }}"

  [multiav.drweb]
  enabled = false
  addr = "multiav-pod.saferwall:10006"

  [multiav.eset]
  enabled = false
  addr = "multiav-pod.saferwall:10007"

  [multiav.fsecure]
  enabled = false
  addr = "multiav-pod.saferwall:10008"

  [multiav.kaspersky]
  enabled = false
  addr = "multiav-pod.saferwall:10009"

  [multiav.symantec]
  enabled = false
  addr = "multiav-pod.saferwall:10010"

  [multiav.sophos]
  enabled = {{ multiav.sophos.enabled | string | lower }}
  addr = "multiav-pod.saferwall:{{ multiav.sophos.port.host }}"

  [multiav.mcafee]
  enabled = false
  addr = "multiav-pod.saferwall:10012"

  [multiav.trendmicro]
  enabled = false
  addr = "multiav-pod.saferwall:10013"

  [multiav.windefender]
  enabled = {{ multiav.windefender.enabled | string | lower }}
  addr = "multiav-pod.saferwall:{{ multiav.windefender.port.host }}"
